@article{sorokinRoleVascularSmooth2020,
  title = {Role of {{Vascular Smooth Muscle Cell Plasticity}} and {{Interactions}} in {{Vessel Wall Inflammation}}},
  author = {Sorokin, Vitaly and Vickneson, Keeran and Kofidis, Theo and Woo, Chin Cheng and Lin, Xiao Yun and Foo, Roger and Shanahan, Catherine M.},
