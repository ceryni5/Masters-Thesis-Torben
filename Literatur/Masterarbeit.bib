@article{robsonPlateletderivedGrowthFactor1992,
  title = {Platelet-Derived Growth Factor {{BB}} for the Treatment of Chronic Pressure Ulcers},
  author = {Robson, M. C. and Phillips, L. G. and Thomason, A. and Robson, L. E. and Pierce, G. F.},
  date = {1992-01-04},
