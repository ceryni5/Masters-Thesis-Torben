@article{zhangSinglecellAtlasChromatin2021,
  title = {A Single-Cell Atlas of Chromatin Accessibility in the Human Genome},
  author = {Zhang, Kai and Hocker, James D. and Miller, Michael and Hou, Xiaomeng and Chiou, Joshua and Poirion, Olivier B. and Qiu, Yunjiang and Li, Yang E. and Gaulton, Kyle J. and Wang, Allen and Preissl, Sebastian and Ren, Bing},
